{
  "name": "@onehilltech/blueprint-mongodb",
  "description": "Blueprint.js module for MongoDB",
  "version": "1.2.4",
  "private": false,
  "main": "./lib",
  "keywords": [
    "blueprint",
    "blueprint-module",
    "mongodb",
    "mongoose"
  ],
  "scripts": {
    "test": "env NODE_ENV=test mocha tests/unit-tests --recursive --exit",
    "cover": "env NODE_ENV=test istanbul cover _mocha tests/unit-tests --recursive",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "postversion": "npm publish",
    "postpublish": "git push origin master & git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onehilltech/blueprint-mongodb.git"
  },
  "dependencies": {
    "@onehilltech/blueprint": "^4.0.5",
    "@onehilltech/dab": "^1.0.9",
    "moment": "^2.19.1"
  },
  "devDependencies": {
    "@onehilltech/blueprint-testing": "^0.7.0",
    "istanbul": "^0.4.5"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  }
}
